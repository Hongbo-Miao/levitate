{
  "name": "@grafana/poc3",
  "version": "0.0.1",
  "private": true,
  "description": "An experimental tool for helping to understand APIs used by plugins and detecting breaking changes.",
  "main": "index.js",
  "scripts": {
    "build": "tsc ./src/bin.ts --m commonjs --outDir ./bin",
    "dev": "tsc-watch ./src/bin.ts --outDir ./bin",
    "dev-compare": "nodemon --exec 'tsc ./src/bin.compare.ts --m commonjs --outDir ./bin && node ./bin/bin.compare.js ./fixtures/bundle-old.ts ./fixtures/bundle-new.ts'",
    "dev-imports": "nodemon --exec 'tsc ./src/bin.imports.ts --m commonjs --outDir ./bin && node ./bin/bin.imports.js'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grafana/data": "8.2.5",
    "chalk": "^5.0.0",
    "colors": "^1.4.0",
    "debug": "^4.3.2",
    "indent-string": "^5.0.0",
    "print-diff": "^1.0.0",
    "typescript": "^4.5.2",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.10",
    "nodemon": "^2.0.15",
    "tsc-watch": "^4.5.0"
  },
  "nodemonConfig": {
    "watch": [
      "*.ts"
    ],
    "ext": "*",
    "events": {
      "start": "cls || clear"
    }
  }
}